{
  "env": {
    "es6": true,
    "browser": true
  },    
  "parserOptions": {
    "sourceType": "module"
  },
  "extends": "eslint:recommended",
  "rules": {
    "no-console": 0,
    "no-unused-vars": 1,
    "no-use-before-define": 0,
    "comma-dangle": 0,
    "no-multi-assign": 0,
    "max-len": 0
  },
  "plugins": ["html", "markdown"]
}
